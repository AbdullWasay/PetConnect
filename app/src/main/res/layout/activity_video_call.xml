<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/remote_user"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView5">

        <FrameLayout
            android:id="@+id/local_user"
            android:layout_width="197dp"
            android:layout_height="209dp"
            android:layout_marginStart="190dp"
            android:layout_marginTop="470dp"
            android:background="@drawable/runded_gray_bg"
            android:fontFamily="sans-serif-medium"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="25dp" />

    </FrameLayout>


    <LinearLayout
        android:id="@+id/textView5"
        android:layout_width="113dp"
        android:layout_height="78dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00:00"
            android:textColor="@color/black"
            android:textSize="25dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/textView13"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/cancelcall"
            android:clickable="true"
            android:gravity="center"
            android:onClick="backbookappointment"
            android:textColor="@color/black"
            android:textSize="25dp" />


        <ImageView
            android:id="@+id/change_camera"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/chnage_camera"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onChangeCameraClick"
            android:textColor="@color/black"
            android:textSize="25dp" />


        <ImageView
            android:id="@+id/camera_hide"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/hide_camera"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onCameraHideClick"
            android:textColor="@color/black"
            android:textSize="25dp" />

        <ImageView
            android:id="@+id/no_voice"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/mute_call"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onNoVoiceClick"
            android:textColor="@color/black"
            android:textSize="25dp" />

    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>